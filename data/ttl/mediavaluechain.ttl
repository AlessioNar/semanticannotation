@prefix : <http://purl.oclc.org/NET/mvco.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.oclc.org/NET/mvco.owl> .

<http://purl.oclc.org/NET/mvco.owl> rdf:type owl:Ontology ;
                                     rdfs:comment "ISO/IEC 21000-19 Media Value Chain Ontology (MVCO)"@en ;
                                     owl:versionInfo "1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.oclc.org/NET/mvco.owl#impliesAlso
:impliesAlso rdf:type owl:AnnotationProperty .


###  http://purl.oclc.org/NET/mvco.owl#rightGivenBy
:rightGivenBy rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.oclc.org/NET/mvco.owl#actOnBehalfOf
:actOnBehalfOf rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :ContentHandler ;
               rdfs:range :User ;
               rdfs:comment "Relates a ContentHandler with the User under the auspices of which the ContentHandler operates."@en ;
               owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#actedBy
:actedBy rdf:type owl:ObjectProperty ;
         rdfs:domain :Action ;
         rdfs:range :User ;
         rdfs:comment "Role who has executed the Action" ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#actedOver
:actedOver rdf:type owl:ObjectProperty ;
           rdfs:domain :Action ;
           rdfs:range :IPEntity ;
           rdfs:comment "Specifies which IPEntity is the object the Action" ;
           owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#acts
:acts rdf:type owl:ObjectProperty ;
      owl:inverseOf :actedBy ;
      rdfs:domain :User ;
      rdfs:range :Action ;
      rdfs:comment "Performance of an Action by a User" ;
      owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#belongsTo
:belongsTo rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain :User ;
           rdfs:range :Collective ;
           rdfs:comment "Relates a User with a Collective"@en ;
           owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#hasRequired
:hasRequired rdf:type owl:ObjectProperty ;
             rdfs:domain :Permission ;
             rdfs:range :Fact ;
             rdfs:comment "For a Permission to be valid, the Fact has to hold" ;
             owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#hasRightsOwner
:hasRightsOwner rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :IPEntity ;
                rdfs:range :User ;
                rdfs:comment "Defines the owner of the Rights over an IP Entity."@en ;
                owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#isMadeUpOf
:isMadeUpOf rdf:type owl:ObjectProperty ;
            rdfs:domain :IPEntity ;
            rdfs:range :IPEntity ;
            rdfs:comment "Relates a composite IPEntity with its constituent IPEntities"@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#isRightsOwnerOf
:isRightsOwnerOf rdf:type owl:ObjectProperty ;
                 owl:inverseOf :hasRightsOwner ;
                 rdfs:domain :User ;
                 rdfs:range :IPEntity ;
                 rdfs:comment "Declares who is the rights owner of this IP Entity" ;
                 owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#issuedBy
:issuedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Permission ;
          rdfs:range :User ;
          rdfs:comment "Declares who has issued a permission" ;
          owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#permitsAction
:permitsAction rdf:type owl:ObjectProperty ;
               rdfs:domain :Permission ;
               rdfs:range :Action ;
               rdfs:comment "Relation used to express the Actions that are allowed to be performed."@en ;
               owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#resultedFrom
:resultedFrom rdf:type owl:ObjectProperty ;
              owl:inverseOf :resultsIn ;
              rdfs:domain :IPEntity ;
              rdfs:range :Action ;
              rdfs:comment "The dependance of one IP Entity on another."@en ;
              owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#resultsIn
:resultsIn rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Action ;
           rdfs:range :IPEntity ;
           rdfs:comment "Declares which IPentity arises as a result of the execution of an Action. It is a functional relation."@en ;
           owl:versionInfo "1.0" .


#################################################################
#    Data properties
#################################################################

###  http://purl.oclc.org/NET/mvco.owl#hasSocialTag
:hasSocialTag rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :IPEntity
                                          :User
                                        )
                          ] ;
              rdfs:range xsd:string ;
              rdfs:comment "Attributes a social tag: text, commentary, critique, synopsis (e.g. FOAF)..." ;
              owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#isDigital
:isDigital rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :IPEntity ;
           rdfs:range xsd:boolean ;
           rdfs:comment "distinguishes between digital management of both digital and non digital IP Entities" ;
           owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#isTrue
:isTrue rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Fact ;
        rdfs:range xsd:boolean ;
        rdfs:comment "Truth of a proposition" ;
        owl:versionInfo "1.0" .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:DatatypeProperty ,
                                                 owl:FunctionalProperty ;
                                        rdfs:range xsd:string ;
                                        rdfs:comment "Title of the ontology" .


###  urn:mpeg:mpeg21:2002:01-DII-NS#RelatedIdentifier
<urn:mpeg:mpeg21:2002:01-DII-NS#RelatedIdentifier> rdf:type owl:DatatypeProperty ,
                                                            owl:FunctionalProperty ;
                                                   rdfs:domain :IPEntity ;
                                                   rdfs:range xsd:string ;
                                                   rdfs:comment "It allows the identification information that is related to a Digital Item (or parts thereof)." .


#################################################################
#    Classes
#################################################################

###  http://purl.oclc.org/NET/mvco.owl#Action
:Action rdf:type owl:Class ;
        rdfs:subClassOf owl:Thing ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :actedBy ;
                          owl:allValuesFrom :User
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :actedBy ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ;
        owl:disjointWith :IPEntity ,
                         :Permission ,
                         :User ;
        rdfs:comment "The exercise of a Right."@en ;
        owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Adaptation
:Adaptation rdf:type owl:Class ;
            rdfs:subClassOf :IPEntity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :resultedFrom ;
                              owl:allValuesFrom [ rdf:type owl:Class ;
                                                  owl:unionOf ( :MakeAdaptation
                                                                :Synchronise
                                                              )
                                                ]
                            ] ;
            owl:disjointWith :Copy ,
                             :Instance ,
                             :Manifestation ,
                             :Product ,
                             :Work ;
            rdfs:comment "A Work that is derived from another Work"@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#AdaptationInstance
:AdaptationInstance rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Instance
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :resultedFrom ;
                                                                 owl:someValuesFrom :MakeAdaptationInstance
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:comment "An object or event which is an example of an Identified Adaptation Manifestation (e.g. a File)"@en ;
                    owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#AdaptationInstanceCopy
:AdaptationInstanceCopy rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( :Copy
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :resultedFrom ;
                                                                     owl:someValuesFrom :MakeAdaptationInstanceCopy
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:comment "A copy of an AdaptationInstance"@en ;
                        owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#AdaptationManifestation
:AdaptationManifestation rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( :Manifestation
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :resultedFrom ;
                                                                      owl:someValuesFrom :MakeAdaptationManifestation
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:comment "An object or event which is an expression of an Adaptation"@en ;
                         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#AdaptationManifestationCopy
:AdaptationManifestationCopy rdf:type owl:Class ;
                             owl:equivalentClass [ owl:intersectionOf ( :Copy
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :resultedFrom ;
                                                                          owl:someValuesFrom :MakeAdaptationManifestationCopy
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ] ;
                             rdfs:comment "A copy of an AdaptationManifestation"@en ;
                             owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Adaptor
:Adaptor rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :acts ;
                               owl:someValuesFrom :MakeAdaptation
                             ] ;
         rdfs:subClassOf :User ;
         rdfs:comment "A User who produces an Adaptation"@en ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Broadcast
:Broadcast rdf:type owl:Class ;
           rdfs:subClassOf :PublicCommunication ;
           rdfs:comment "The Function that Delivers Content to a Device in a point-to-multipoint modality"@en ;
           owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Collective
:Collective rdf:type owl:Class ;
            rdfs:subClassOf :User ;
            rdfs:comment "Set of two or more Users."@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#ContentHandler
:ContentHandler rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :actOnBehalfOf ;
                                      owl:someValuesFrom :User
                                    ] ;
                rdfs:subClassOf :User ;
                rdfs:comment "User who is appointed to act on Content on behalf and within the scope and responsability of another UserÂ´s rights."@en ;
                owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Copy
:Copy rdf:type owl:Class ;
      rdfs:subClassOf :IPEntity ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :resultedFrom ;
                        owl:someValuesFrom :MakeCopy
                      ] ;
      owl:disjointWith :Instance ,
                       :Manifestation ,
                       :Product ,
                       :Work ;
      rdfs:comment "A mechanical reproduction of analogue or digital representations of a given IP Entity. In the case of digital Copies the result is virtually identical while in the case of analogue Copies the results can vary considerably in quality."@en ;
      owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#CopyrightExceptionFact
:CopyrightExceptionFact rdf:type owl:Class ;
                        rdfs:subClassOf :Fact ;
                        rdfs:comment "Fact related to the invokation of a CopyrightException" ;
                        owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#CopyrightExceptionPermission
:CopyrightExceptionPermission rdf:type owl:Class ;
                              rdfs:subClassOf :Permission ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :hasRequired ;
                                                owl:someValuesFrom :CopyrightExceptionFact
                                              ] ;
                              rdfs:comment "Permission to invoke one right exceptionally." ;
                              owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#CreateWork
:CreateWork rdf:type owl:Class ;
            rdfs:subClassOf :Action ,
                            [ rdf:type owl:Class ;
                              owl:complementOf [ rdf:type owl:Restriction ;
                                                 owl:onProperty :actedOver ;
                                                 owl:someValuesFrom :IPEntity
                                               ]
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :resultsIn ;
                              owl:allValuesFrom :Work
                            ] ;
            owl:disjointWith :Distribute ,
                             :EndUserAction ,
                             :MakeAdaptation ,
                             :MakeCopy ,
                             :MakeInstance ,
                             :MakeManifestation ,
                             :Produce ,
                             :PublicCommunication ,
                             :Synchronise ;
            rdfs:comment "The action of creating a Work."@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Creator
:Creator rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :acts ;
                               owl:someValuesFrom :CreateWork
                             ] ;
         rdfs:subClassOf :User ;
         rdfs:comment "A User who generates a Work and makes its first Manifestation, also referred to as author"@en ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Distribute
:Distribute rdf:type owl:Class ;
            rdfs:subClassOf :Action ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :actedOver ;
                              owl:allValuesFrom :Product
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :resultsIn ;
                              owl:cardinality "0"^^xsd:nonNegativeInteger
                            ] ;
            owl:disjointWith :EndUserAction ,
                             :MakeAdaptation ,
                             :MakeCopy ,
                             :MakeInstance ,
                             :MakeManifestation ,
                             :Produce ,
                             :PublicCommunication ,
                             :Synchronise ;
            :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Creator"^^xsd:anyURI ,
                          "http://purl.oclc.org/NET/mvco.owl#Producer"^^xsd:anyURI ;
            rdfs:comment "The Function of selling, renting and lending"@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Distributor
:Distributor rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :acts ;
                                   owl:someValuesFrom :Distribute
                                 ] ;
             rdfs:subClassOf :User ;
             rdfs:comment "A User who distributes a Product"@en ;
             owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Download
:Download rdf:type owl:Class ;
          rdfs:subClassOf :PublicCommunication ;
          rdfs:comment "The Action of transferring a file or program from a central computer to a smaller computer or to a computer at a remote location"@en ;
          owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#EndUser
:EndUser rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :acts ;
                               owl:someValuesFrom :EndUserAction
                             ] ;
         rdfs:subClassOf :User ;
         rdfs:comment "A User in a Value-Chain who ultimately consumes Content."@en ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#EndUserAction
:EndUserAction rdf:type owl:Class ;
               rdfs:subClassOf :Action ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :actedOver ;
                                 owl:allValuesFrom :Product
                               ] ;
               owl:disjointWith :MakeAdaptation ,
                                :MakeCopy ,
                                :MakeInstance ,
                                :MakeManifestation ,
                                :Produce ,
                                :PublicCommunication ,
                                :Synchronise ;
               :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Distributor"^^xsd:anyURI ;
               rdfs:comment "Action performed by an EndUser" ;
               owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Fact
:Fact rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :isTrue ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
      owl:disjointWith :User ;
      rdfs:comment "Positive proposition." ;
      owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#IPEntity
:IPEntity rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRightsOwner ;
                            owl:allValuesFrom :User
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isMadeUpOf ;
                            owl:allValuesFrom :IPEntity
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :resultedFrom ;
                            owl:allValuesFrom :Action
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRightsOwner ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          owl:disjointWith :Permission ,
                           :User ;
          rdfs:comment "Types of IP Represented as Content: Work, Adaptation, Manifestation, Instance..."@en ;
          owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Instance
:Instance rdf:type owl:Class ;
          rdfs:subClassOf :IPEntity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :resultedFrom ;
                            owl:someValuesFrom :MakeInstance
                          ] ;
          owl:disjointWith :Manifestation ,
                           :Product ,
                           :Work ;
          rdfs:comment "An object or event which is an example of an Identified Manifestation (e.g. a File)"@en ;
          owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Instantiator
:Instantiator rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :acts ;
                                    owl:someValuesFrom :MakeInstance
                                  ] ;
              rdfs:subClassOf :User ;
              rdfs:comment "A User who interprets a Manifestation of a Work making an Instance"@en ;
              owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeAdaptation
:MakeAdaptation rdf:type owl:Class ;
                rdfs:subClassOf :Action ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :actedOver ;
                                  owl:allValuesFrom [ rdf:type owl:Class ;
                                                      owl:unionOf ( :Adaptation
                                                                    :Work
                                                                  )
                                                    ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :resultsIn ;
                                  owl:allValuesFrom :Adaptation
                                ] ;
                owl:disjointWith :MakeCopy ,
                                 :MakeInstance ,
                                 :MakeManifestation ,
                                 :Produce ,
                                 :PublicCommunication ,
                                 :Synchronise ;
                :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Adaptor"^^xsd:anyURI ,
                              "http://purl.oclc.org/NET/mvco.owl#Creator"^^xsd:anyURI ;
                rdfs:comment "The Action of making an Adaptation"@en ;
                owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeAdaptationInstance
:MakeAdaptationInstance rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( :MakeInstance
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :actedOver ;
                                                                     owl:someValuesFrom :AdaptationManifestation
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty :resultsIn ;
                                          owl:allValuesFrom :AdaptationInstance
                                        ] ;
                        rdfs:comment "The Action of making an Instance from an AdaptationManifestation"@en ;
                        owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeAdaptationInstanceCopy
:MakeAdaptationInstanceCopy rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( :MakeCopy
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :actedOver ;
                                                                         owl:someValuesFrom :AdaptationInstance
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf [ rdf:type owl:Restriction ;
                                              owl:onProperty :resultsIn ;
                                              owl:allValuesFrom :AdaptationInstanceCopy
                                            ] ;
                            rdfs:comment "The Action of making an AdaptationInstanceCopy"@en ;
                            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeAdaptationManifestation
:MakeAdaptationManifestation rdf:type owl:Class ;
                             owl:equivalentClass [ owl:intersectionOf ( :MakeManifestation
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :actedOver ;
                                                                          owl:someValuesFrom :Adaptation
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ] ;
                             rdfs:subClassOf [ rdf:type owl:Restriction ;
                                               owl:onProperty :resultsIn ;
                                               owl:allValuesFrom :AdaptationManifestation
                                             ] ;
                             rdfs:comment "The action of making an AdaptationManifestation from an Adaptation."@en ;
                             owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeAdaptationManifestationCopy
:MakeAdaptationManifestationCopy rdf:type owl:Class ;
                                 owl:equivalentClass [ owl:intersectionOf ( :MakeCopy
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :actedOver ;
                                                                              owl:someValuesFrom :AdaptationManifestation
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ] ;
                                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                   owl:onProperty :resultsIn ;
                                                   owl:allValuesFrom :AdaptationManifestationCopy
                                                 ] ;
                                 rdfs:comment "The Action of making an AdaptationManifestationCopy" ;
                                 owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeCopy
:MakeCopy rdf:type owl:Class ;
          rdfs:subClassOf :Action ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :actedOver ;
                            owl:allValuesFrom [ rdf:type owl:Class ;
                                                owl:unionOf ( :Instance
                                                              :Manifestation
                                                            )
                                              ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :resultsIn ;
                            owl:allValuesFrom :Copy
                          ] ;
          owl:disjointWith :MakeInstance ,
                           :MakeManifestation ,
                           :Produce ,
                           :PublicCommunication ,
                           :Synchronise ;
          :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Creator"^^xsd:anyURI ,
                        "http://purl.oclc.org/NET/mvco.owl#Instantiator"^^xsd:anyURI ;
          rdfs:comment "The Action of making a Copy"@en ;
          owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeInstance
:MakeInstance rdf:type owl:Class ;
              rdfs:subClassOf :Action ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :actedOver ;
                                owl:someValuesFrom :Manifestation
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :resultsIn ;
                                owl:someValuesFrom :Instance
                              ] ;
              owl:disjointWith :MakeManifestation ,
                               :Produce ,
                               :PublicCommunication ,
                               :Synchronise ;
              rdfs:comment "The Action of making an Instance from a Manifestation."@en ;
              owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeManifestation
:MakeManifestation rdf:type owl:Class ;
                   rdfs:subClassOf :Action ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :actedOver ;
                                     owl:someValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( :Adaptation
                                                                        :Work
                                                                      )
                                                        ]
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :resultsIn ;
                                     owl:allValuesFrom :Manifestation
                                   ] ;
                   owl:disjointWith :Produce ,
                                    :PublicCommunication ,
                                    :Synchronise ;
                   rdfs:comment "The action of making a Manifestation."@en ;
                   owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeWorkInstance
:MakeWorkInstance rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :MakeInstance
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :actedOver ;
                                                               owl:someValuesFrom :WorkManifestation
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :resultsIn ;
                                    owl:allValuesFrom :WorkInstance
                                  ] ;
                  rdfs:comment "The Action of making an Instance from a Work Manifestation."@en ;
                  owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeWorkInstanceCopy
:MakeWorkInstanceCopy rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( :MakeCopy
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :actedOver ;
                                                                   owl:someValuesFrom :WorkInstance
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :resultsIn ;
                                        owl:allValuesFrom :WorkInstanceCopy
                                      ] ;
                      rdfs:comment "The Action of making a WorkInstanceCopy"@en ;
                      owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeWorkManifestation
:MakeWorkManifestation rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :MakeManifestation
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :actedOver ;
                                                                    owl:someValuesFrom :Work
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty :resultsIn ;
                                         owl:allValuesFrom :WorkManifestation
                                       ] ;
                       rdfs:comment "The action of making a Manifestation from Work."@en ;
                       owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MakeWorkManifestationCopy
:MakeWorkManifestationCopy rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( :MakeCopy
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty :actedOver ;
                                                                        owl:someValuesFrom :WorkManifestation
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty :resultsIn ;
                                             owl:allValuesFrom :WorkManifestationCopy
                                           ] ;
                           rdfs:comment "The Action of making a WorkManifestationCopy"@en ;
                           owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Manifestation
:Manifestation rdf:type owl:Class ;
               rdfs:subClassOf :IPEntity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :resultedFrom ;
                                 owl:someValuesFrom :MakeManifestation
                               ] ;
               owl:disjointWith :Product ,
                                :Work ;
               rdfs:comment "An object or event which is an expression of a Work."@en ;
               owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#ModifyCopy
:ModifyCopy rdf:type owl:Class ;
            rdfs:subClassOf :EndUserAction ;
            rdfs:comment "Action of modifying a copy."@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#MoveContent
:MoveContent rdf:type owl:Class ;
             rdfs:subClassOf :EndUserAction ;
             rdfs:comment "The action of moving the location of a content"@en ;
             owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Permission
:Permission rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :permitsAction ;
                              owl:someValuesFrom :Action
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasRequired ;
                              owl:allValuesFrom :Fact
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :issuedBy ;
                              owl:allValuesFrom :User
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :permitsAction ;
                              owl:allValuesFrom :Action
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :issuedBy ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            owl:disjointWith :User ;
            rdfs:comment "Authorisation from one RightsOwner to one or more Users to perform one or more Actions on a given IPEntity."@en ;
            owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Produce
:Produce rdf:type owl:Class ;
         rdfs:subClassOf :Action ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :actedOver ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:unionOf ( :Copy
                                                             :Instance
                                                             :Manifestation
                                                           )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :resultsIn ;
                           owl:allValuesFrom :Product
                         ] ;
         owl:disjointWith :PublicCommunication ,
                          :Synchronise ;
         :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Creator"^^xsd:anyURI ,
                       "http://purl.oclc.org/NET/mvco.owl#Instantiator"^^xsd:anyURI ;
         rdfs:comment "The Function of making Products"@en ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Producer
:Producer rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :acts ;
                                owl:someValuesFrom :Produce
                              ] ;
          rdfs:subClassOf :User ;
          rdfs:comment "A User who produces a Product from an Instance."@en ;
          owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Product
:Product rdf:type owl:Class ;
         rdfs:subClassOf :IPEntity ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :resultedFrom ;
                           owl:someValuesFrom :Produce
                         ] ;
         owl:disjointWith :Work ;
         rdfs:comment "A Content Item that adds value to IP Entities by including them with an appropriate Licence for the purpose of Publishing"@en ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#PublicCommunication
:PublicCommunication rdf:type owl:Class ;
                     rdfs:subClassOf :Action ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :actedOver ;
                                       owl:allValuesFrom [ rdf:type owl:Class ;
                                                           owl:unionOf ( :Copy
                                                                         :Instance
                                                                         :Product
                                                                       )
                                                         ]
                                     ] ;
                     owl:disjointWith :Synchronise ;
                     :impliesAlso "http://purl.oclc.org/NET/mvco.owl#Render"^^xsd:anyURI ;
                     :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Creator"^^xsd:anyURI ,
                                   "http://purl.oclc.org/NET/mvco.owl#Producer"^^xsd:anyURI ;
                     rdfs:comment "The Function of publicly displaying/performing, e.g. live performance, radio, television, internet streaming, multicast of Instances and Manifestations, and download"@en ;
                     owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Render
:Render rdf:type owl:Class ;
        rdfs:subClassOf :EndUserAction ;
        rdfs:comment "The Action of generating a human-perceivable signal"@en ;
        owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Stream
:Stream rdf:type owl:Class ;
        rdfs:subClassOf :PublicCommunication ;
        rdfs:comment "The Function of Delivering Content to a Device where the transferred Content is Processed for Rendering only and not Stored"@en ;
        owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Synchronise
:Synchronise rdf:type owl:Class ;
             rdfs:subClassOf :Action ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :actedOver ;
                               owl:allValuesFrom :Work
                             ] ;
             :impliesAlso "http://purl.oclc.org/NET/mvco.owl#ModifyCopy"^^xsd:anyURI ,
                          "http://purl.oclc.org/NET/mvco.owl#Render"^^xsd:anyURI ;
             :rightGivenBy "http://purl.oclc.org/NET/mvco.owl#Creator"^^xsd:anyURI ;
             rdfs:comment "The Action of concurrently performing/displaying two distinct IP Entities each for a different human sense e.g. text and audio or video and song"@en ;
             owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#UseData
:UseData rdf:type owl:Class ;
         owl:disjointWith :User ;
         rdfs:comment "Data documenting the functions Actions performed by a Device User on a content item and the associated context" ;
         owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#User
:User rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :acts ;
                        owl:allValuesFrom :Action
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :belongsTo ;
                        owl:allValuesFrom :Collective
                      ] ;
      rdfs:comment "Any person or legal entity in a Value-Chain connecting (and including) Creator and End-User."@en ;
      owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#Work
:Work rdf:type owl:Class ;
      rdfs:subClassOf :IPEntity ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :resultedFrom ;
                        owl:allValuesFrom :CreateWork
                      ] ;
      rdfs:comment "A creation that retains intellectual or artistic attributes independently of its Manifestations"@en ;
      owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#WorkInstance
:WorkInstance rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Instance
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :resultedFrom ;
                                                           owl:someValuesFrom :MakeWorkInstance
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "An object or event which is an example of an Identified Manifestation of a Work (e.g. a File)"@en ;
              owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#WorkInstanceCopy
:WorkInstanceCopy rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Copy
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :resultedFrom ;
                                                               owl:someValuesFrom :MakeWorkInstanceCopy
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:comment "A copy of a WorkInstance"@en ;
                  owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#WorkManifestation
:WorkManifestation rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Manifestation
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :resultedFrom ;
                                                                owl:someValuesFrom :MakeWorkManifestation
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:comment "An object or event which is an expression of a Manifestation of a Work"@en ;
                   owl:versionInfo "1.0" .


###  http://purl.oclc.org/NET/mvco.owl#WorkManifestationCopy
:WorkManifestationCopy rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Copy
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :resultedFrom ;
                                                                    owl:someValuesFrom :MakeWorkManifestationCopy
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:comment "A copy of a WorkManifestation."@en ;
                       owl:versionInfo "1.0" .


###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://purl.oclc.org/NET/mvco.owl#Anonymous
:Anonymous rdf:type owl:NamedIndividual ,
                    :User .